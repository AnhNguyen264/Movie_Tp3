@model ObjectiveVM

@{
    ViewData["Title"] = @Locals["Title"];

}

<h1 style="padding-top:40px">Nom d'objective: @Model.objectives.NomObjective</h1>

<h3> Les vendeurs impliqués dans le projet</h3>
<div class="row">
    @*  POUR CHAQUE ZOMBIE:*@
    @foreach (var item in Model.vendeurList)
    {

        <partial name="_vendeurPartiel" model="item" />


    }
</div>

<div>
    <a class="btn btn-secondary" asp-action="Index">@Locals["BackToList"]</a>
</div>
